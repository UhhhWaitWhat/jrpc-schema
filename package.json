{
  "name": "jrpc-schema",
  "version": "0.0.3",
  "description": "Parses a json schema into a simple usable api object",
  "author": "Florian Albertz",
  "license": "BSD-3-Clause",
  "main": "lib/jrpc-schema.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Paulavery/jrpc-schema.git"
  },
  "scripts": {
    "prepublish": "make lint"
  },
  "devDependencies": {
    "eslint": "^0.21.1",
    "version-bump": "^0.1.0"
  },
  "dependencies": {
    "skeemas": "^1.1.6"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "curly": [
        2,
        "multi-line"
      ],
      "quotes": [
        2,
        "single"
      ]
    },
    "ecmaFeatures": {
      "modules": true
    }
  }
}
